{
  "name": "dev-env--nodejs",
  "version": "1.0.0",
  "description": "Ready-to-code environment",
  "main": "index.js",
  "author": "LongLH <i.me.mine@luchoanglong.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/node": "7.23.9",
    "@babel/preset-env": "7.24.3",
    "html-webpack-plugin": "5.6.0",
    "nodemon": "3.1.0",
    "webpack": "5.93.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "react": "18.3.1"
  },
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node -L src/index.js --watch src --ignore src/client",
    "build": "webpack --config webpack.config.js",
    "serve": "nodemon --exec \"webpack serve --config webpack.config.js\" --watch webpack.config.js"
  }
}
